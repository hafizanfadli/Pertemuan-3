#include <iostream>
using namespace std;

int main() {
    
    int bb,tb,tbhideal;
    
    tbhideal = bb / (tb * tb);
	
    cout<<"Berat Badan : ";
    cin>>bb;
    cout<<"Tinggi Badan : ";
    cin>>tb;
        if (bb>=50 && tb>=160){
            cout << "Tubuh kamu ideal";
        }
        else{
            cout << "Tubuh Kamu terlalu kurus";
        }
        
    return 0;
	
}
